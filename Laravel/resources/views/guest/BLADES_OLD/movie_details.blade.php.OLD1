        <script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script>
        <script type='text/javascript'>
            $(document).ready(function () {

                $("a[data-page='home']").addClass("active");

                $("#homepageCarousel").carousel();

                // this creates the popover the movies
                $('[data-toggle="popover"]').popover();
                if($(window).width() < 769){
                    $('[data-toggle="popover"]').popover("disable");
                }

                $('[data-toggle="offcanvas"]').click(function () {
                    $('.row-offcanvas').toggleClass('active');
                });

                $('.dropdown-toggle').dropdown();

                $(".dynamic-slider").carousel();

                function updateNav(slider, tid){
                    var totalItems = slider.find('.item').length;
                    var currentIndex = slider.find('div.active').index() + 1;

                    if(currentIndex == 1)
                        $("#btnPrev_" + tid).hide();
                    else $("#btnPrev_" + tid).show();

                    if(currentIndex == totalItems)
                        $("#btnNext_" + tid).hide();
                    else $("#btnNext_" + tid).show();
                }

                //TODO: Ajax calls for carousel population
                //slide.bs.carousel = as soon as event occurs, slid.bs.carousel = once slide transition ends.
                $(".dynamic-slider").on('slid.bs.carousel', function(e){
                    var self = $(this);
                    var tid = parseInt($(this).attr("data-tid"));
                    updateNav(self, tid);
                });

                $(".dynamic-slider").on('slide.bs.carousel', function(e){
                    var self = $(this);

                    //Only fetch AJAX data if we're not complete & we're at least half way through
                    // the carousel (or we're just starting to page)
                    var totalItems = self.find('.item').length;
                    var currentIndex = self.find('div.active').index() + 1;

                    if(self.data("complete") === true || self.data("processing") === true || (currentIndex != 1 && currentIndex < totalItems/2))
                        return;

                    var lastItem = self.find(".item").last();
                    var offset = parseInt(self.attr("data-offset"));
                    var tid = parseInt(self.attr("data-tid"));
                    var filterPath = self.attr("data-filter");

                    switch(filterPath){
                        case "subscription":
                            filter = 1;
                            break;
                        case "premium":
                            filter = 2;
                            break;
                        default:
                            filter = 0;
                    }


                    self.data("processing", true);
                    $.ajax({
                        url: "/fetch/" + tid + "/" + offset + "/" + filter,
                        method: "GET",
                        data: {},
                        success: function(data){
                            if(data.length != 0) {
                                var slidesPerPage = 6;
                                var width = $(window).width();
                                //  determine slidesPerPage based on screen width
                                switch(true){
                                    case (width > 768):
                                        slidesPerPage = 6;
                                        break;
                                    case (width > 639):
                                        slidesPerPage = 4;
                                        break;
                                    case (width > 359):
                                        slidesPerPage = 3;
                                        break;
                                    default:
                                        slidesPerPage = 2;
                                        break;
                                }
                                var tabIndex = offset - slidesPerPage;
                                var leftRightCounter = slidesPerPage + slidesPerPage;
                                var length = data.length;
                                for (var index = 0; index < length; index++) {
                                    //add a new div.item after the carousels last item, and set it to lastItem
                                    if(index % slidesPerPage === 0)
                                        lastItem = $("<div class='item'></div>").insertAfter(lastItem);
                                    lastItem.append("" +
                                        "<div class='movie_item  col-md-2 col-sm-2 col-xs-4'>" +
                                        "<a href='/movie/" + data[index].nid + "/watch-" + data[index].node_title.replace(" ", "-") + "' alt='watch " + data[index].node_title + " online'>" + //TODO: Verify this matches with seoLink helper function
                                        "<img class='cover_img' alt='" + data[index].node_title + "' src='" + data[index].files_filepath + "' " +
                                        "role='button' data-toggle='popover' data-trigger='hover' data-animation='true' tabindex='" + tabIndex + "' data-placement='" + ((leftRightCounter % 6) < 3 ? 'right' : 'left') + "' " +
                                        "data-title=\"" +
                                        "<span class='movie-title'>" + data[index].node_title + "</span>" +
                                        "<div class='movie-info-row'>" +
                                        "<span class='movie-mpaa-rating'>" + data[index].mpaa_rating_value + "</span>" +
                                        "<span class='movie-year'></span>" +
                                        "<span class='movie-runtime'>" + data[index].runtime_value + "</span>" +
                                        "<span class='movie-starts'></span>" +
                                        "</div>" +
                                        "\" " +
                                        "data-content=\"" +
                                        "<div class='movie-description'>" +
                                        "<p>" + ((data[index].description.length >= 300) ? $.trim(data[index].description).substring(0, 300).split(" ").slice(0, -1).join(" ") + "..." : data[index].description).replace(/"/g, "&quot;") + "</p>" +
                                        "</div>" +
                                        "\" " +
                                        "data-container='body' data-html='true' " + ">" +
                                        "</a>" +
                                        "<div class='movie_title'>" +
                                        ((data[index].premium == 1) ? "<img alt='premium' src='/img/movie-premium.png' align='right'/>" : "<img alt='subscription' src='/img/movie-subscribe.png' align='right'/>") +
                                        "<a href='/movie/" + data[index].nid + "/watch-" + data[index].node_title.replace(" ", "-") + "' alt='watch " + data[index].node_title + " online' style='white-space:nowrap;'>" +
                                        data[index].node_title +
                                        "</a>" +
                                        "</div>" +
                                        "</div>");

                                    tabIndex ++;
                                    leftRightCounter++;
                                }

                                // this creates the popover the movies
                                if($(window).width() > 768){
                                    $('[data-toggle="popover"]').popover();
                                }

                                offset += length;
                                self.attr("data-offset", offset);
                            }
                            else self.data("complete", true);

                            updateNav(self, tid);
                        },
                        error: function(){},
                        complete: function(){
                            self.data("processing", false);
                        }
                    });
                });

            });
        </script>
    </div>
</div>
<script>
    $(function(){
        $(".navbar-toggle").click(function(){
            var body = $("body");
            var docWrapper = $("#documentWrapper");
            var footer = $("footer");
            var mobileMenu = $("#mobile-menu");
            var slideTime = 250;
            var menuWidth = 250;

            body.toggleClass("mobile-menu-open");
            //  open menu
            if(body.hasClass("mobile-menu-open")){
                docWrapper.stop().animate({"left":"-" + menuWidth + "px"}, slideTime);
                footer.stop().animate({"left":"-" + menuWidth + "px"}, slideTime);
                mobileMenu.stop().animate({"right":0}, slideTime);
                //  close menu
            }else{
                docWrapper.stop().animate({"left":0}, slideTime);
                footer.stop().animate({"left":0}, slideTime);
                mobileMenu.stop().animate({"right":"-" + menuWidth + "px"}, slideTime);
                mobileMenu.children("ul").find("li.has-submenu").find("ul.open").removeClass("open").animate({"right":"-" + menuWidth + "px"}, slideTime);
            }

            //  open submenu
            mobileMenu.children("ul").find("li.has-submenu").find("a").click(function(){
                var submenu = $(this).siblings("ul");
                submenu.addClass("open");
                $(this).siblings("ul").stop().animate({"right":0}, slideTime);
            });
            //  close submenu
            mobileMenu.find("li.back").find("a").click(function(){
                var submenu = $(this);
                while(!submenu.is("ul")){
                    submenu = submenu.parent();
                }
                submenu.removeClass("open");
                submenu.stop().animate({"right":"-" + menuWidth + "px"}, slideTime);
            });

            docWrapper.off("click");
            $("body.mobile-menu-open").find("#documentWrapper").click(function(){
                $(this).off("click");
                $(".navbar-toggle").click();
            });
        });
    });
</script>
